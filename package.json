{
  "name": "f22-react-boilerplate",
  "version": "1.0.0",
  "description": "Boiler plate for new react projects in f22labs",
  "main": "./src/index.js",
  "scripts": {
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "build:debug": "yarn debug  --env.mode production",
    "start:debug": "yarn debug  --env.mode development",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "build": "yarn webpack  --env.mode production",
    "start": "yarn webpack-dev-server  --env.mode development --open ",
    "start:hot": "yarn webpack-dev-server  --env.mode development --hot --open",
    "prod:typescript": "yarn  build  --env.presets typescript",
    "build:analyze": "yarn build  --env.presets analyze",
    "build:compress": "yarn build  --env.presets compress",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "format": "pretty-quick",
    "lint": "eslint ./",
    "lint:fix": "yarn lint --fix"
  },
  "keywords": [],
  "author": "Ganesh R",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^5.9.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "react-testing-library": "^5.2.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.2",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-dotenv-plugin": "^2.1.0",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "react-hot-loader": "^4.3.12"
  },
  "browserslist": [
    ">0.2%",
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
